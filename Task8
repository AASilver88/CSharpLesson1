// Программа, которая на вход принимает число (N), а на выходе показывает все чётные числа от 1 до N.

Console.WriteLine("Введите число N, и программа выдаствсе чётные числа от 1 до N.");
int N = int.Parse(Console.ReadLine());

for (int x = 1; x < N; x++)       
if (x%2==0)            
Console.WriteLine(x);

if (N < 1)
{
    Console.WriteLine("Введенное число мкньше 1");
}
