// Программа, которая на вход принимает три числа и выдаёт, максимальное из этих чисел.

Console.WriteLine("Введите три числа и программа назовет максимальное число");

Console.WriteLine("Введите первое число");
int a = int.Parse(Console.ReadLine());
Console.WriteLine("Введите второе число");
int b = int.Parse(Console.ReadLine());
Console.WriteLine("Введите третье число");
int c = int.Parse(Console.ReadLine());

if (a >= b)
{
    if (a >= c)
    {
        Console.WriteLine("Максимальное число max=" + a);
    }
}
if (b > a)
{
    if (b >= c)
    {
        Console.WriteLine("Максимальное число max=" + b);
    }
}
if (c > a)
{
    if (c > b)
    {
        Console.WriteLine("Максимальное число max=" + c);
    }
}
